# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Darwin-iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Darwin-iOS
  pod 'FBSDKLoginKit'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'
  
  target 'Darwin-iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Darwin-iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
